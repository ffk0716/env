#!/usr/bin/env python3

import os
import re

p = re.compile(r'(\d{4}-\d{2}-\d{2} \d{2}.\d{2}.\d{2})(.*)')
p2 = re.compile(r'(\d{4}-\d{2}-\d{2})')

a = os.listdir()
for f in a:
    if os.path.isfile(f):
        m = p.match(f)
        if m:
            m = p2.match(f)
            t = m.group(1)
            if not os.path.exists(t):
                os.makedirs(t)
            os.rename(f, os.path.join(t, f))
            print(f'move {f} to {t}')
